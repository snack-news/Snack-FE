{
  "name": "snack-fe",
  "version": "0.0.1",
  "description": "Snack News FE",
  "main": "index.js",
  "scripts": {
    "init": "lerna bootstrap --hoist",
    "build": "webpack",
    "start:dev": "webpack-dev-server",
    "test": "jest --coverage --watchAll",
    "ts-compile-check": "tsc -p ./tsconfig.noEmit.json"
  },
  "author": "",
  "license": "UNLICENSED",
  "husky": {
    "hooks": {
      "pre-commit": "npm run ts-compile-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "jest --bail --findRelatedTests",
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "@snack-fe/react-hello": "file:packages/react-hello",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "lerna": "^3.13.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@snack-fe/eslint-config-snack": "file:packages/eslint-config-snack",
    "@types/jest": "^24.0.3",
    "@types/react-test-renderer": "^16.8.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "eslint": "^5.15.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-watch-typeahead": "^0.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.8.3",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
