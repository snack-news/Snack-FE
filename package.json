{
  "name": "snack-fe",
  "version": "0.0.1",
  "description": "Snack News FE",
  "main": "index.js",
  "scripts": {
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "webpack --config ./n-client/webpack.config.js",
    "dev": "concurrently \"npm run build:client -- -w\" \"npm run build:server -- -w\" \"npm run server\"",
    "storybook": "start-storybook -c ./storybook/config --port 60453",
    "build:server": "webpack --config ./n-server/webpack.config.js",
    "server": "node ./n-server/dist/index.js",
    "postinstall": "link-module-alias",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true"
  },
  "_moduleAliases": {
    "~root": ".",
    "~client": "./client/src",
    "~nclient": "./n-client/src",
    "~server": "./server/src",
    "~storybook": "./storybook",
    "~tools": "./tools"
  },
