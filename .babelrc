{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": false,
        "targets": {
          "chrome": "77",
          // "ie": "11"
        },
      },
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": {
          "version": 3,
          // "proposals": true,
        },
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
  ],
  "inputSourceMap": true,
  "sourceMaps": true
}